<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Technical Service Request Number -->
        <record id="seq_technical_service_request" model="ir.sequence">
            <field name="name">Technical Service Request</field>
            <field name="code">technical.service.request</field>
            <field name="prefix">REQ-%(year)s-</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>